<Window x:Class="UtraSoundTouchUI.Views.TouchUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UtraSoundTouchUI.Views"
             mc:Ignorable="d" 
             Title="Touch UI"
             d:DesignHeight="300" d:DesignWidth="1366" Height="780">
    <Grid>
        <TabControl Style="{DynamicResource TabControlStyle1}" >
            <TabItem Header="B" Margin="0" Style="{DynamicResource TabItemStyle1}" Foreground="{StaticResource ApplicationForeground}">
               <local:BMode/>
            </TabItem>
            <TabItem Header="M" Margin="0" Style="{DynamicResource TabItemStyle1}" Foreground="{StaticResource ApplicationForeground}">
               <local:MMode/>
            </TabItem>
            
        </TabControl>
    </Grid>
</Window>
